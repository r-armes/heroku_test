<p id="notice"><%= notice %></p>

<p>
  <strong>This post was created by:</strong>
  <%= @post.user.first_name %>
</p>

<p>
  <strong>Title:</strong>
  <%= @post.title %>
</p>

<p>
  <strong>Subitle:</strong>
  <%= @post.subtitle %>
</p>

<p>
  <strong>Content:</strong>
  <%= @post.content %>
</p>

<%= link_to 'Edit', edit_post_path(@post) %> |
<%= link_to 'Back', posts_path %>
